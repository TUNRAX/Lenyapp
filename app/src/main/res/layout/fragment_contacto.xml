<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactoFragment"
    android:elevation="3dp"
    android:visibility="gone"
    android:outlineProvider="bounds"
    android:paddingBottom="2dp">

    <!-- TODO: Update blank fragment layout -->

    <ImageButton
        android:id="@+id/btnCall1"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:background="#0000"
        android:src="@drawable/phonereceiver" />

    <ImageButton
        android:id="@+id/btnSms1"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="15dp"
        android:background="#0000"
        android:src="@drawable/chatbubbles" />

    <TextView
        android:id="@+id/txtNum1"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/roboto"
        android:text="(+56) (9) 978 334 66"
        android:textSize="16sp" />

    <ImageButton
        android:id="@+id/btnCall2"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="55dp"
        android:background="#0000"
        android:src="@drawable/phonereceiver" />

    <ImageButton
        android:id="@+id/btnSms2"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="55dp"
        android:background="#0000"
        android:src="@drawable/chatbubbles" />

    <TextView
        android:id="@+id/txtNum2"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="55dp"
        android:fontFamily="@font/roboto"
        android:text="(+56) (9) 813 726 54"
        android:textSize="16sp" />

</FrameLayout>